<% provide(:title, 'Create a new Team') %>
<h1>New Team</h1>

<div class="row">
    <div class="span6 offset3">
        <%= form_for(@team) do |f| %>
            <%= render 'fields', f: f %>
            <%= f.submit "Create", class: "btn btn-large btn-primary" %>
        <% end %>
    </div>
</div>
