<% provide(:title, 'Add new member') %>
<h1>New Member</h1>

<div class="row">
	<div class="span6 offset3">
				
    <%= form_for("users", controller: "teams", action: "add_members") do |f| %>
			<%= f.label :email %>
			<%= f.text_field :email %>
      <%= f.submit "Create", class: "btn btn-large btn-primary" %>
    <% end %>
	</div>
</div>
